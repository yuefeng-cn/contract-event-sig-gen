{
  "manifest_version": 2,
  "name": "智能合约事件签名生成工具",
  "version": "1.0",
  "description": "从智能合约中的事件定义生成签名",
  "permissions": ["activeTab", "clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
